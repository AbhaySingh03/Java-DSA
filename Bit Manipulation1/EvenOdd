package BitManipulation;

public class EvenOdd {

	static int evenodd(int n) {
		int res = n & 1;
		return res == 0 ? 1:-1;
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int n = 99;
		int res = evenodd(n);
		if(res == 1) {
		System.out.println("even");
	}else {
		System.out.println("odd");
	 }
  }
}
